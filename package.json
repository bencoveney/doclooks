{
  "name": "doclooks",
  "version": "0.0.1",
  "description": "Documentation styles",
  "main": "dist",
  "scripts": {
    "build": "node-sass -o dist src/main",
    "develop": "npm run build -- --watch",
    "docs-build": "npm run --prefix gatsby build",
    "docs-develop": "npm run --prefix gatsby develop",
    "test": "npm run build && npm run docs-build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bencoveney/doclooks.git"
  },
  "keywords": [
    "Documentation",
    "CSS",
    "SCSS",
    "BEM"
  ],
  "author": "Ben Coveney",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bencoveney/doclooks/issues"
  },
  "homepage": "https://github.com/bencoveney/doclooks#readme",
  "devDependencies": {
    "node-sass": "^4.7.2"
  }
}
